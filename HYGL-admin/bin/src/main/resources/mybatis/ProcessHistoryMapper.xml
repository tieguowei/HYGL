   <!-- 列表查询条件 -->
   
  <sql id="dynamicPageWhere">
  
    <if test="processName != null">
  		and process_name like CONCAT('%',#{processName},'%')
 	</if>
 	<if test="processType != null">
  		and process_type = #{processType}
 	</if>
    <if test="minCreateTime != null">
      <![CDATA[
 		AND DATE_FORMAT(create_time ,'%Y-%m-%d') >= #{minCreateTime}
 		]]>
    </if>
    <if test="maxCreateTime != null">
      <![CDATA[
 		AND DATE_FORMAT(create_time ,'%Y-%m-%d') <= #{maxCreateTime}
 	]]>
    </if>
 
  </sql>
  
   <!--流程历史列表分页 -->
	<select id="findAllRetMapByPage" resultType="hashmap" parameterType="hashmap">
		SELECT
		  id,
		  process_id,
		   DATE_FORMAT(create_time,'%Y-%m-%d')   create_time,
		  process_name,
		  process_sketch,
		 (SELECT sd.dict_name FROM system_dict sd WHERE sd.dict_type = 'PROCESS_TYPE' AND sd.dict_code =process_type ) process_type ,
		  DATE_FORMAT(start_run_time,'%H:%i:%s') start_run_time,
		  DATE_FORMAT(end_run_time,'%H:%i:%s') end_run_time,
		  SEC_TO_TIME(TIMESTAMPDIFF(second, start_run_time,end_run_time)) run_duration,
		  version
	FROM
		t_process_history
			where 1=1
		<include refid="dynamicPageWhere" />
		order by create_time desc 
		limit #{startIndex},#{pageSize}
  </select>
  
   <!-- 列表分页总数 -->
	<select id="findAllByPageCount" resultType="long" parameterType="hashmap">
		SELECT count(*) FROM t_process_history
		where 1=1
		<include refid="dynamicPageWhere" />
	</select>
  
